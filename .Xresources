! set the font
URxvt.font: neep-alt-18

! get rid of that Windows98 scroll bar
URxvt.scrollBar: false

! set transparent background
urxvt.depth: 32

! forground is the main font color
! special
*.foreground:   #a7a7a7
*.background:   [80]#170f0d
*.cursorColor:  #8a8986

! black
*.color0:       #28211c
*.color8:       #666666

! red
*.color1:       #cf6a4c
*.color9:       #cf6a4c

! green
*.color2:       #54be0d
*.color10:      #54be0d

! yellow
*.color3:       #f9ee98
*.color11:      #f9ee98

! blue
*.color4:       #5ea6ea
*.color12:      #5ea6ea

! magenta
*.color5:       #9b859d
*.color13:      #9b859d

! cyan
*.color6:       #4587e4
*.color14:      #afc4db

! white
*.color7:       #8a8986
*.color15:      #baae9e


